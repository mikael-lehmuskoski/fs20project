{
  "extends": [
    "react-app",
    "airbnb",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-case-declarations": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
