{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
  "extends": [
      "airbnb",
      "prettier",
      "eslint:recommended"
      //"plugin:node/recommended"
  ],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2020, 
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars": "warn",
      "no-console": "off"
  }
}